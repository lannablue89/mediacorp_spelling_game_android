<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:clock="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <com.xenione.digit.TabDigit
        android:id="@+id/charHighSecond"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        clock:backgroundColor="@color/black"
        clock:padding="@dimen/medium_margin"
        clock:textColor="@color/white"
        clock:textSize="80sp" />

    <com.xenione.digit.TabDigit
        android:id="@+id/charLowSecond"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        clock:backgroundColor="@color/black"
        clock:padding="@dimen/medium_margin"
        clock:textColor="@color/white"
        clock:textSize="80sp" />

</merge>